{
  "name": "simplerangeslider",
  "version": "1.0.0",
  "browserslist": [
    "last 3 version"
  ],
  "scripts": {
    "test": "karma start",
    "css:dev": "postcss ./src/Style/style.css -o ./dist/SimpleRangeSlider.css --watch -env development",
    "css:prod": "postcss ./src/Style/style.css -o ./dist/SimpleRangeSlider.css -env development && postcss ./src/Style/style.css -o ./dist/SimpleRangeSlider.min.css --env production",
    "plugin:dev": "webpack --config webpack.config.js --mode development --watch",
    "plugin:prod": "webpack --config webpack.config.js --mode development && webpack --mode production",
    "plugin:start": "webpack-dev-server --open",
    "demo": "copy dist\\SimpleRangeSlider.min.css docs && webpack --config webpackfile.js --mode development --watch",
    "eslint": "eslint src\\plugin\\**\\*.ts --fix",
    "eslint:test": "eslint src\\test\\**\\*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n0sfer666/FSD-Task-4-v2.git"
  },
  "keywords": [
    "range",
    "slider",
    "rangeslider"
  ],
  "author": "Khasanov Petr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/n0sfer666/FSD-Task-4-v2/issues"
  },
  "homepage": "https://github.com/n0sfer666/FSD-Task-4-v2#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "@types/jquery": "^3.3.31",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "autoprefixer": "^9.7.3",
    "copy": "^0.3.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jasmine-core": "^3.5.0",
    "jquery": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "postcss-cli": "^6.1.3",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-simple-vars": "^5.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  }
}
